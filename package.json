{
  "name": "template-node-ci",
  "version": "1.0.1",
  "description": "That's a simple solution to start APIs in nodejs if you don't want to download all dependencies every time when you start some new project.",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "npx rimraf dist && npx tsc -p tsconfig-build.json && npx prisma generate",
    "test": "vitest run",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "dev": "tsx app.ts",
    "start": "node dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/mongodb": "^4.0.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@vitest/coverage-istanbul": "^0.32.4",
    "eslint": "^8.43.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prisma": "^5.1.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vitest": "^0.32.4"
  },
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "axios": "^1.5.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongodb": "^5.7.0",
    "stripe": "^12.12.0"
  }
}
